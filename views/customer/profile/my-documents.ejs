<div class="tab-pane fade" id="documents" role="tabpanel">
  <div class="row justify-content-center">
    <form class="ctp-contact-form documents-form">
      <div class="col-md-12 mb-4">
        <h5>My Documents</h5>
      </div>
      <div class="row">
        <div class="col-sm-2 doc_img text-center">
          <img src="/images/document-icon.png" class="mb-2" style="width: 100px" />
          <% if(user.pancardDocument) { %>
            <a href="/uploads/<%= user.pancardDocument %>" class="text-primary" target="_blank">View <i class="fa fa-eye"></i></a>
          <% } %>
        </div>
        <div class="col-sm-4">
          <div class="form-group">
            <label>Pan Card </label>
            <input type="file" name="pancardDocument" id="pancardDocument" class="form-control" />
          </div>
        </div>
        <div class="col-sm-1"></div>
        <div class="col-sm-2 doc_img text-center">
          <img src="/images/document-icon.png" class="mb-2" style="width: 100px" />
          <% if(user.aadharCardDocument) { %>
            <a href="/uploads/<%= user.aadharCardDocument %>" class="text-primary" target="_blank">View <i class="fa fa-eye"></i></a>
          <% } %>
        </div>
        <div class="col-sm-4">
          <div class="form-group">
            <label>Aadhar Card</label>
            <input type="file" name="aadharCardDocument" id="aadharCardDocument" class="form-control" />
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-3">
          <button type="submit" class="btn btn-primary">Save</button>
          <div id="msgSubmit" class="h3 text-center hidden"></div>
        </div>
      </div>
      <div class="clearfix"></div>
    </form>
  </div>
</div>
