    <script>
      const dashboard = document.getElementById('dashboard')

      function validateFileSize(input) {
        const maxFileSizeMB = 1;
        const file = input.files[0];

        if (file) {
          const fileSizeMB = file.size / (1024 * 1024);
          if (fileSizeMB > maxFileSizeMB) {
            alert(`File size exceeds ${maxFileSizeMB} MB. Please choose a smaller file.`);
            input.value = '';
          }
        }
      }

      const pancard = document.getElementById('pancardAdd')
      const pancardError = document.getElementById('panNumberError')
      const pattern = new RegExp("[A-Za-z]{5}[0-9]{4}[A-Za-z]{1}");

      pancard.addEventListener('blur', () => {
        console.log(pancard.value);
        if (pattern.test(pancard.value)) {
          console.log("Valid PAN card number.");
          pancardError.innerText = ""

        } else {
          console.log("Invalid PAN card number.");
          pancardError.innerText = "Invalid PAN"
        }
      });
    </script>

    <script>
      function formatInput(displayInput, hiddenElement) {
        let hiddenInput = document.getElementById(hiddenElement);
        let x = displayInput.value.replace(/,/g, '');

        if (x === '') {
          hiddenInput.value = '';
          displayInput.value = '';
        } else {
          hiddenInput.value = x;
          console.log('input', x, parseInt(x).toLocaleString('en-IN'));
          displayInput.value = parseInt(x).toLocaleString('en-IN');
        }
      }
    </script>


