            <div class="tab-pane fade" id="security" role="tabpanel" aria-labelledby="#security-tab">
              <div class=" justify-content-center">

                <form class="ctp-contact-form profile-form">
                  <div class="col-md-12 mb-4">
                    <h5>My Profile</h5>
                  </div>
                  <div class="row">
                    <div class="col-sm-4">
                      <div class="form-group">
                        <label>Profile</label>
                        <% if (user.profile) { %>
                          <div class="m-2 mb-2">
                            <img src="/uploads/<%= user.profile %>" alt="user" style="max-height: 150px;">
                          </div>
                          <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                            <b>Replace :</b>
                            <% } %>
                              <% } %>
                                <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                                  <input type="file" name="profile" id="profile" data-error="Please upload your profile"
                                    accept=".jpg,.jpeg,.png">
                                  <% } %>
                      </div>
                    </div>
                    <div class="col-sm-4">
                      <div class="form-group">
                        <b>Cheque Document :</b>
                        <% if (user.bankChequeDocument) { %>
                          <div class="mt-2 mb-2">
                            <a href="/uploads/<%= user.bankChequeDocument %>" target="_blank">
                              View Saved Cheque
                            </a>
                          </div>
                          <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                            <b>Replace :</b>
                            <% } %>
                              <% } %>
                                <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                                  <input type="file" name="bankChequeDocument" id="bankChequeDocument"
                                    data-error="Please upload your profile" accept=".pdf">
                                  <% } %>
                      </div>
                      <div class="form-group">
                        <b>Pancard Document :</b>
                        <% if (user.pancardDocument) { %>
                          <div class="mt-2 mb-2">
                            <a href="/uploads/<%= user.pancardDocument %>" target="_blank">
                              View Saved Pancard
                            </a>
                          </div>
                          <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                            <b>Replace :</b>
                            <% } %>
                              <% } %>
                                <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                                  <input type="file" name="pancardDocument" id="pancardDocument"
                                    data-error="Please upload your profile" accept=".jpg,.jpeg,.png,.pdf">
                                  <% } %>
                      </div>
                    </div>
                    <div class="col-sm-4">
                      <div class="form-group">
                        <b>Aadhar Card Document :</b>
                        <% if (user.aadharCardDocument) { %>
                          <div class="mt-2 mb-2">
                            <a href="/uploads/<%= user.aadharCardDocument %>" target="_blank">
                              View Saved Aadhar
                            </a>
                          </div>
                          <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                            <b>Replace :</b>
                            <% } %>
                              <% } %>
                                <% if (user.status !=='Approved' || user.status !=='Rejected' ) { %>
                                  <input type="file" name="aadharCardDocument" id="aadharCardDocument"
                                    data-error="Please upload your profile" accept=".jpg,.jpeg,.png,.pdf">
                                  <% } %>
                      </div>
                      <div class="col-sm-6">
                        <div class="form-group" style="max-height: 20px;">
                          <b>Status :- </b>
                          <%= user.status %>
                            <br>
                            <% if (user.remark && user.remark !=null ) { %>
                              <b>Remark :- </b>
                              <%= user.remark %>
                                <% } %>
                                  <br>
                                  <% if (user.chanelCode && user.chanelCode !=null ) { %>
                                    <b>Chanel Code :- </b>
                                    <%= user.chanelCode %>
                                      <% } %>
                                        <!-- <div class="help-block with-errors"></div> -->
                        </div>
                      </div>
                      <!-- <div class="col-sm-4">
                        <div class="form-group">
                        </div>
                      </div> -->
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Name</label>
                        <input type="text" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="name" id="name"
                        class="form-control" value="<%= user.name %>"
                          data-error="Please enter your name" placeholder="Sergio Laughlin">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Email</label>
                        <input type="email" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="email" id="email"
                        class="form-control" value="<%= user.email %>"
                          data-error="Please enter your email" placeholder="luvion@gmail.com">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Phone</label>
                        <input readonly="true" placeholder="enter 10 digit phone" type="text"
                          <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="phone" id="phone"
                        class="form-control" value="<%= user.phone %>"
                          data-error="Please enter your surname" placeholder="George" pattern="[0-9]{10}">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>

                    <!-- <div class="col-sm-6">
                      <div class="form-group">
                        <label>Gender</label>
                        <select class="nice-select" name="gender">
                          <option>Male</option>
                          <option>Female</option>
                        </select>
                        <div class="help-block with-errors"></div>
                      </div>
                    </div> -->

                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>DOB</label>
                        <input type="date" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="dob" id="dob"
                        class="form-control"
                        value="<%= new Date(user.dob).toISOString().substr(0, 10) %>"
                          data-error="Please enter your surname" placeholder="George">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <!-- <div class="col-sm-6">
                      <div class="form-group">
                        <label>Marital Status</label>
                        <select class="nice-select" name="maritalStatus">
                          <option value="Married">Married</option>
                          <option value="Unmarried">Unmarried</option>
                        </select>
                        <div class="help-block with-errors"></div>
                      </div>
                    </div> -->
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>PAN No</label>
                        <input type="text" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="pancard" id="pancard"
                        required="true" value="<%= user.pancard %>"
                          data-error="Please enter your number" class="form-control" placeholder="Enter"
                          pattern="[A-Za-z]{5}[0-9]{4}[A-Za-z]{1}">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Aadhar No</label>
                        <input type="text" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="aadhar" id="aadhar"
                        required="true" value="<%= user.aadhar %>"
                          data-error="Please enter your number" class="form-control" placeholder="Enter"
                          pattern="[0-9]{12}">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Area Pin</label>
                        <input type="text" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="pincode" id="pincode"
                        required="true" value="<%= user.pincode %>"
                          class="form-control" placeholder="Enter">
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>State</label>
                        <input type="text" readonly name="state" id="state" required="true" value="<%= user.state %>"
                          class="form-control" placeholder="Enter">
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>City</label>
                        <input type="text" readonly name="city" id="city" required="true" value="<%= user.city %>"
                          class="form-control" placeholder="Enter">
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <label>Address</label>
                        <input type="text" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> name="address" id="address"
                        required="true" value="<%= user.address %>"
                          data-error="Please enter your number" class="form-control" placeholder="Enter">
                          <div class="help-block with-errors"></div>
                      </div>
                    </div>
                    <!-- <div class="col-sm-6">
                      <div class="form-group">
                        <label>PAN No</label>
                        <input type="text" name="pancard" id="pancard" required="true" value="<%= user.pancard %>"
                          data-error="Please enter your number" class="form-control" placeholder="Enter">
                        <div class="help-block with-errors"></div>
                      </div>
                    </div> -->
                    <br />
                    <div class="col-sm-7">
                      <button type="submit" <%= (user.status==='Approved' || user.status==='Rejected') ? "disabled" : '' %> class="btn btn-primary
                        w-50">Update Profile</button>
                      <div id="msgSubmit" class="h3 text-center hidden"></div>
                    </div>
                  </div>
                  <div class="clearfix"></div>
                </form>

              </div>
            </div>


            <div class="tab-pane fade" id="leadH" role="tabpanel" aria-labelledby="#leadH-tab">
              <div class=" justify-content-center">
                <div class="col-lg-12 col-sm-12">
                  <h5 class="mb-5">My Enquiry</h5>
                  <div class="row">
                    <div class="col-sm-2">
