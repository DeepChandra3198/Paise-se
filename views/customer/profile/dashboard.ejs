<%- include('../partials/header') %>

  <style>
    .main-content-label {
      font-size: x-large;
      background: linear-gradient(to right, #7779c3, #0dbad4);
      color: transparent;
    }
  </style>

  <section class="ptb-70 my-account-pg mt-5 grey-bg">
    <marquee>
      <h6 class="text-success">
        <%= offers?.[0]?.offer ? offers?.[0]?.offer : "" %>
          <img src="/uploads/<%= offers?.[0]?.photo %>" alt="offer" style="max-height: 40px;">
      </h6>
    </marquee>
    <div class="container">
      <div class="ctp-key-features-tabs">
        <h5 class="mb-3">Customer My Account</h5>

        <div class="row col-sm d-flex row-wrap-reverse">
          <ul class="nav nav-tabs" id="myTab" role="tablist" style="width: 90vw;">
            <li class="nav-item col" role="presentation">
              <a class="nav-link <% (tab ==='2') ? 'active' : '' %>" id="security-tab" data-bs-toggle="tab"
                href="#security" role="tab" aria-controls="security" aria-selected="false" tabindex="-1"><i
                  class="fas fa-chevron-left"></i> My
                Profile</a>
            </li>
            <li class="nav-item col" role="presentation">
              <a class="nav-link <% (tab ==='1') ? 'active' : '' %>" id="dashboard-tab" data-bs-toggle="tab"
                href="#dashboard" role="tab" aria-controls="dashboard" aria-selected="true"><i
                  class="fas fa-chevron-left"></i>Dashboard</a>
            </li>
            <li class="nav-item col" role="presentation">
              <a class="nav-link <% (tab ==='3') ? 'active' : '' %>" id="lead-tab" data-bs-toggle="tab" href="#lead"
                role="tab" aria-controls="lead" aria-selected="false" tabindex="-1"
                style="pointer-events: <%= user.status !== 'Approved' ? 'none' : 'auto' %>;"><i
                  class="fas fa-chevron-left"></i> Add Leads</a>
            </li>
            <li class="nav-item col" role="presentation">
              <a class="nav-link" id="leadH-tab" data-bs-toggle="tab" href="#leadH" role="tab" aria-controls="leadH"
                aria-selected="false" tabindex="-1"
                style="pointer-events: <%= user.status !== 'Approved' ? 'none' : 'auto' %>;">
                <i class="fas fa-chevron-left"></i> Leads History
              </a>
            </li>
            <li class="nav-item col" role="presentation">
              <a class="nav-link" id="paymentR-tab" data-bs-toggle="tab" href="#paymentR" role="tab"
                aria-controls="paymentR" aria-selected="false" tabindex="-1"
                style="pointer-events: <%= user.status !== 'Approved' ? 'none' : 'auto' %>;"><i
                  class="fas fa-chevron-left"></i> Payment
                Recon</a>
            </li>
            <li class="nav-item col">
              <a class="nav-link" id="refferal-tab" data-bs-toggle="tab" href="javascript:;"
                onclick="document.querySelector('.logout-form').submit()" aria-controls="refferal"><i
                  class="fas fa-sign-out-alt"></i>Logout</a>
            </li>
          </ul>

          <div class="tab-content mt-5" id="myTabContent" style="width: 90vw;">

